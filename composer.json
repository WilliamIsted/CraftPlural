{
    "name": "williamisted/plural",
    "description": "Simple plural plugin for Craft - Usage: {{ entries | length | plural('result') }}",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "plural"
    ],
    "support": {
        "docs": "https://github.com/WilliamIsted/CraftPlural/blob/master/README.md",
        "issues": "https://github.com/WilliamIsted/CraftPlural/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "William Isted",
            "homepage": "https://william.isted.me/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "williamisted\\plural\\": "src/"
        }
    },
    "extra": {
        "name": "Plural",
        "handle": "plural",
        "developer": "William Isted",
        "developerUrl": "https://william.isted.me/",
        "documentationUrl": "https://github.com/WilliamIsted/CraftPlural/blob/master/README.md",
        "changelogUrl": "https://github.com/WilliamIsted/CraftPlural/master/CHANGELOG.md"
    }
}
